<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.vinsan"
version="1.0.0"
name="VINSAN Movies"
provider-name="vinsan">
<requires>
  <import addon="xbmc.python" version="3.0.0"/>
  <import addon="script.module.codequick" version="1.0.2"/>
</requires>
<extension point="xbmc.python.pluginsource" library="addon.py">
  <provides>video</provides>
</extension>
<extension point="xbmc.addon.metadata">
  <summary lang="en">0gomovies Kodi Video Plugin</summary>
  <description lang="en_GB">0gomovies Kodi video plugin</description>
  <disclaimer lang="en_GB">Free movies provided by 0gomovies</disclaimer>
  <assets>
    <icon>film.png</icon>
    <fanart>fanart.jpg</fanart>
  </assets>
  <news>Updated with latest artwork metadata</news>
</extension>
</addon>

<addon id="vinsanrepository" name="VINSAN Repository" version="1.0.0" provider-name="vinsan">
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/vinsan-repo/vinsanrepository/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/vinsan-repo/vinsanrepository/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/vinsan-repo/vinsanrepository/master/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>VINSAN Repository</summary>
        <description>KODI Repository for addons</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
